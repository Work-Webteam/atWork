{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/animated_scroll_to_top": "^1.2",
        "drupal/background_image": "^1.3",
        "drupal/background_image_formatter": "^1.3",
        "drupal/bootstrap": "^3.11",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/colorbox": "^1.4",
        "drupal/colorbox_inline": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/crop": "^1.5",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.1",
        "drupal/flag": "^4.0@alpha",
        "drupal/forward": "^2.1",
        "drupal/h5p": "^1.0@RC",
        "drupal/image_widget_crop": "^2.2",
        "drupal/insert": "^1.0",
        "drupal/libraries": "^3.0@alpha",
        "drupal/like_dislike": "^1.1",
        "drupal/likeit": "^1.0",
        "drupal/module_filter": "^3.1",
        "drupal/poll": "^1.2",
        "drupal/postal_code": "1.x-dev",
        "drupal/printfriendly": "^3.3",
        "drupal/rate": "^1.0",
        "drupal/scheduler": "^1.0",
        "drupal/tether_stats": "^1.1",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/video_embed_field": "^2.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/viewsreference": "^1.2",
        "drush/drush": "^9.3",
        "symfony/var-dumper": "^3.4",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
	"patches": {
	    "drupal/like_dislike": {
	        "Fixing error where like/dislike module was searching for language": "https://www.drupal.org/files/issues/2832520-php-notice-undefined-index-in-multilingual.patch"
	    }
	}
    }
}
