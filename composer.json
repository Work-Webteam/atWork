{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/activity": "1.x-dev",
        "drupal/admin_toolbar": "^1.24",
        "drupal/animated_scroll_to_top": "^1.2",
        "drupal/background_image": "^1.3",
        "drupal/background_image_formatter": "^1.3",
        "drupal/bootstrap": "^3.13",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/chosen": "^2.6",
        "drupal/colorbox": "^1.4",
        "drupal/colorbox_inline": "^1.0",
        "drupal/console": "^1.8",
        "drupal/core": "^8.6.0",
        "drupal/crop": "^1.5",
        "drupal/devel": "^1.2",
        "drupal/draggable_dashboard": "^1.1",
        "drupal/ds": "^3.1",
        "drupal/flag": "^4.0@alpha",
        "drupal/forward": "^2.1",
        "drupal/group": "^1.0",
        "drupal/h5p": "^1.0@RC",
        "drupal/image_widget_crop": "^2.2",
        "drupal/insert": "^1.0",
        "drupal/libraries": "^3.0@alpha",
        "drupal/like_dislike": "^1.1",
        "drupal/likeit": "^1.0",
        "drupal/message_digest": "^1.0@beta",
        "drupal/message_subscribe": "^1.0@beta",
        "drupal/message_ui": "^1.0@beta",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/module_filter": "^3.1",
        "drupal/panelizer": "^4.1",
        "drupal/panels": "^4.3",
        "drupal/pathauto": "^1.3",
        "drupal/photos": "^4.1",
        "drupal/plupload": "1.x-dev",
        "drupal/poll": "^1.2",
        "drupal/postal_code": "1.x-dev",
        "drupal/printfriendly": "^3.3",
        "drupal/rate": "^1.0",
        "drupal/redirect": "^1.3",
        "drupal/reroute_email": "^1.0",
        "drupal/rules": "^3.0@alpha",
        "drupal/scheduler": "^1.0",
        "drupal/search_api_sorts": "^1.0@beta",
        "drupal/search_api_stats": "1.x-dev",
        "drupal/search_api_stats_block": "1.x-dev",
        "drupal/synonyms": "^1.0@alpha",
        "drupal/taxonomy_import": "^1.0",
        "drupal/taxonomy_manager": "^1.0@alpha",
        "drupal/taxonomy_permissions": "1.x-dev",
        "drupal/term_csv_export_import": "^2.0",
        "drupal/term_merge_manager": "^1.0@beta",
        "drupal/tether_stats": "^1.1",
        "drupal/token": "^1.5",
        "drupal/token_filter": "^1.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/video_embed_field": "^2.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/viewsreference": "^1.2",
        "drupal/webform": "^5.0@RC",
        "drush/drush": "^9.3",
        "harvesthq/chosen": "^1.8",
        "symfony/var-dumper": "^3.4",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
	"patches": {
	    "drupal/like_dislike": {
	        "Fixing error where like/dislike module was searching for language": "https://www.drupal.org/files/issues/2832520-php-notice-undefined-index-in-multilingual.patch"
	    },
	    "drupal/devel": {
	        "Fixing kint issue": "https://www.drupal.org/files/issues/devel.ksm_.2857361.18.patch"
      },
	    "drupal/mimemail": {
	        "Implements send mail function for mimemail plugin": "https://www.drupal.org/files/issues/2018-07-25/2938963-10.patch"
	    },
	    "drupal/group": {
	        "Adds parent-group token for nodes": "https://www.drupal.org/files/issues/2018-12-19/group-2774827-41-gnode-tokens.patch"
	    }
	}
    }
}
